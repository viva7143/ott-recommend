{
  "manifest_version": 3,
  "name": "OTT Top Series",
  "version": "1.0",
  "description": "Displays the top OTT series of the week",
  "icons": {
    "128": "icon.png"
  },
  "permissions": ["activeTab"],
  "host_permissions": ["http://localhost:5000/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
